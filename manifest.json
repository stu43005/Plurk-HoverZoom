{
    "manifest_version": 2,
    "name": "Plurk-HoverZoom",
    "version": "2.9.2",
    "description": "Show super-sized avatars and photos when you run your cursor over the thumbnail.",
    "icons": {
        "128": "128.png",
        "16": "16.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://www.plurk.com/*",
                "https://www.plurk.com/*"
            ],
            "js": [
                "jquery-2.0.2.min.js",
                "content-script.js"
            ]
        }
    ],
    "permissions": [
        "http://www.plurk.com/",
        "https://www.plurk.com/"
    ],
    "web_accessible_resources": [
        "jquery-2.0.2.js",
        "jquery-2.0.2.min.map"
    ]
}